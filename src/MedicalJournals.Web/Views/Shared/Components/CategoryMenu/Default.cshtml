@model IEnumerable<string>

<li class="dropdown">
    <a asp-area="" asp-controller="Journals" asp-action="Index" class="dropdown-toggle" data-toggle="dropdown">Journals <b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var categoryName in Model)
        {
            <li>
                <a asp-area="" asp-controller="Journals" asp-action="Browse" asp-route-Category="@categoryName">@categoryName</a>
            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Journals" asp-action="Index">More...</a>
        </li>
    </ul>
</li>